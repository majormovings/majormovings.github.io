                <ul>
                    <li>Appliances</li>
                    <li>Billiard Tables</li>
                    <li>Clocks</li>
                    <li>Furniture</li>
                    <li>Fitness Equipment</li>
                    <li>TVs</li>
                    <li>Playground Equipment</li>
                    <li>Pianos</li>
                    <li>Museum Art</li>
                    <li>Safes</li>
                    <li>Home Theatre Systems</li>
                    <li>Hot Tubs/Spas</li>
                    <li>Smart Beds/Mattresses</li>
                </ul>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <%- include('../_components/_get_quotes.ejs') %>
            </div>
        </div>

    </div>
    <%- include('../_components/_accordion.ejs') %>
    <%- include('../_components/_footer.ejs') %>
</main>

<script src="/assets/css/bootstrap/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="/assets/scripts/main.js"></script>
